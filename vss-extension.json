{
    "manifestVersion": 1,
    "id": "gpt-pr-review-jp",
    "name": "GPT Pull Request Review JP",
    "version": "1.0.10",
    "publisher": "kohta-jrits",
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "日本語でPull Requestの変更内容をGPTでレビューするAzure Pipelinesタスク",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "tags": [
        "OpenAI",
        "GPT",
        "pullRequest",
        "review"
    ],
    "files": [
        {
            "path": "GPTPullRequestReview"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/kohta-jrits/azure-pipeline-gpt-pr-review-jp"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "contributions": [
        {
            "id": "GPTPullRequestReviewJP",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "GPTPullRequestReview"
            }
        }
    ]
